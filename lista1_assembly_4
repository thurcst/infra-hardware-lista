.data
    stringinha:         .asciiz "orr o"
    comeco:              .word 0
    final:             .word 0
    fim_string:         .word 0

.text
    la x11, comeco
    la x12, final
    la x9, stringinha
    lw x8, fim_string
    la x7, stringinha
	xor a0, a0, a0

    run:
        lb x11 0 x9 #referencial do comeco
    
    
    while:
        lb x11 0 x7
        lb x12 0 x9 #referencial do final
        beq x12, x8, next_step # if x12 == x8 then next_step
        addi x9, x9, 1 # x12 = x12 + 1
        beq zero, zero, while

    next_step:
        addi x9 x9 -1
        lb x12 0 x9 #referencial do a2

    compare_registers:
        lb x12 0 x9 #
        lb x11 0 x7 #
        bne x11, x12, print_n # if x11 != x12 then target
        beq zero zero reducao_reg

    reducao_reg:
        bge x7, x9, print_s # if t0 >= t1 then target
        addi x7 x7 1
        addi x9 x9 -1
        beq zero zero compare_registers
    
    print_n:
        addi a0 a0 0
        beq zero, zero, exit
   
    print_s:
        addi a0 a0 1
    
    exit:
